{
  "name": "TikTok Phone Login with Coordinates",
  "description": "Example showing coordinate-based clicks",
  "version": "1.0",
  "parameters": [
    {
      "name": "phoneNumber",
      "type": "string",
      "required": true,
      "description": "Phone number without country code"
    }
  ],
  "steps": [
    {
      "id": "click_by_coordinates",
      "type": "tap",
      "coordinates": {
        "x": 360,
        "y": 640
      },
      "description": "Click at specific coordinates (example: center of 720x1280 screen)"
    },
    {
      "id": "click_profile_tab",
      "type": "tap",
      "coordinates": {
        "x": 648,
        "y": 1220
      },
      "description": "Click Profile tab (bottom right on 720x1280)"
    },
    {
      "id": "click_with_offset",
      "type": "tap",
      "coordinates": {
        "x": 360,
        "y": 500,
        "offset": {
          "x": -10,
          "y": 5
        }
      },
      "description": "Click with offset from base coordinates"
    },
    {
      "id": "swipe_example",
      "type": "swipe",
      "coordinates": {
        "startX": 360,
        "startY": 800,
        "endX": 360,
        "endY": 400,
        "duration": 500
      },
      "description": "Swipe up from middle of screen"
    },
    {
      "id": "long_press_example",
      "type": "long_press",
      "coordinates": {
        "x": 360,
        "y": 640,
        "duration": 1000
      },
      "description": "Long press for 1 second"
    }
  ]
} 